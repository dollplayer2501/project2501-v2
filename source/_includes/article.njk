<html lang="{{ site.language }}">

    <head>
        <title>{{ site.name | escape }}</title>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
        <link rel="stylesheet" href="/assets/styles/main.css" />
        <link rel="icon" href="/favicon.ico" />
        <script>
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());
            gtag('config', '{{ site.googleAnalytics4 }}');
        </script>
    </head>

    <body class="is-preload">

        <!-- Header -->
            <header id="header">
                <h1>{{ site.name | escape }}</h1>
                <p>
                    <img src="/images/{{ site.author.avator }}" style="width:48px;"><br clear="all" />
                    <span style="font-size:1.2em;font-weight:bold;">{{ site.author.name | escape }}</span><br />
                    <span>{{ site.description1 | escape }}</span><br />
                    <span style="font-size:0.9em;">{{ site.description2 | escape }}</span>
                </p>
                <h2>More information ?</h2>
                <p>
                    <a href="{{ site.more.url }}">
                        <span style="font-weight:bold;">{{ site.more.name | escape }}</span>
                        <span style="font-size:0.75em;">
                            /
                            {{ site.more.description | escape }}
                        </span>
                    </a>
                </p>
            </header>

        <!-- Footer -->
            <footer id="footer">
                <ul class="icons">
{%- for item in socials %}
                    <li><a href="{{ item.url }}" class="icon brands {{ item.fontawesome }}"><span class="label">{{ item.name | escape }}</span></a></li>
{%- endfor %}
                </ul>
                <ul class="copyright">
{%- for item in thanks %}
                    <li><a href="{{ item.url }}">{{ item.name | escape }}</a></li>
{%- endfor %}
                    <li>
                        GitHub, <a href="{{ site.github }}/commit/{{ buildInfo.hash }}">last built on {{ buildInfo.time.formatted }} {{ buildInfo.hash }}</a>
                    </li>
                </ul>
            </footer>

        <!-- Scripts -->
            <script src="/assets/scripts/main.js"></script>

    </body>
</html>
