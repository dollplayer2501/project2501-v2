{
  "name": "project2501-v2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "develop:check": "ncu",
    "develop:update": "ncu -u",
    "develop:clean": "rimraf ./_develop",
    "develop:gulp:css": "cross-env NODE_ENV=develop npx gulp css",
    "develop:gulp:js": "cross-env NODE_ENV=develop npx gulp js",
    "develop:gulp:webp": "cross-env NODE_ENV=develop npx gulp webp",
    "develop:gulp:build": "cross-env NODE_ENV=develop npx gulp build",
    "develop:gulp:watch": "cross-env NODE_ENV=develop npx gulp watch",
    "develop:eleventy:build": "cross-env NODE_ENV=develop npx @11ty/eleventy",
    "develop:eleventy:watch": "cross-env NODE_ENV=develop npx @11ty/eleventy --serve --port=8081",
    "develop:build": "npm-run-all --sequential develop:clean --parallel develop:gulp:build develop:eleventy:build develop:node:webp:build",
    "develop:watch": "npm-run-all --sequential develop:clean develop:gulp:build develop:node:webp:build --parallel develop:gulp:watch develop:eleventy:watch develop:node:webp:watch",
    "develop:node:webp:build": "cross-env NODE_ENV=develop node scripts/image-min.mjs",
    "develop:node:webp:watch": "onchange \"source/images/**/*.{jpg,jpeg,png}\" -- npm run develop:node:webp:build",
    "product:clean": "rimraf ./_production",
    "product:gulp:build": "cross-env NODE_ENV=production npx gulp build",
    "product:eleventy:build": "cross-env NODE_ENV=production npx @11ty/eleventy",
    "product:build": "npm-run-all --sequential product:clean --parallel product:gulp:build product:eleventy:build product:node:webp:build",
    "product:serve": "cd ./_production && npx serve -p 3001",
    "product:node:webp:build": "cross-env NODE_ENV=product node scripts/image-min.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "cross-env": "^7.0.3",
    "gulp": "^4.0.2",
    "gulp-mode": "^1.1.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "html-minifier": "^4.0.0",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "npm-check-updates": "^16.14.6",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "serve": "^14.2.1"
  }
}
